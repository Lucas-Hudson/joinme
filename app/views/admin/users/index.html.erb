<main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
<h2 class="text-center">Manage Users</h2>
<br>
<br>
<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th>id</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
        <th>Created at</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user|%>
        <tr>
        <td><%=user.id%></td>
        <td><%=user.first_name%></a></td>
        <td><%=user.last_name%></td>
        <td><%=user.email%></td>
        <td><%=user.created_at%></td>
        <td> <a href="<%=admin_user_path(user.id)%>">See profile</a> </td>
      </tr>
    <% end %>
      </tbody>
  </table>
</div>
</main>
</div>
</div>